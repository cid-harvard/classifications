DOWNLOAD = PYTHONPATH=../../../ python -B download_sheets.py
CLEAN = PYTHONPATH=../../../ python -B clean_livestock.py

LIVESTOCK = in/Livestock_Hierarchy.tsv in/Livestock_Names.tsv
AGPRODUCTS = in/AgProducts_Hierarchy.tsv in/AgProducts_Names.tsv
FARMTYPE = in/FarmType_Hierarchy.tsv in/FarmType_Names.tsv

: |> $(DOWNLOAD) |> $(LIVESTOCK) $(AGPRODUCTS) $(FARMTYPE)
: $(LIVESTOCK) |> $(CLEAN) |> out/livestock.csv out/livestock.dta
